{
	"name": "frontend-myself",
	"version": "1.0.0",
	"description": "Webpack",
	"private": true,
	"scripts": {
		"start": "set NODE_ENV=development&&webpack serve --open",
		"dev": "set NODE_ENV=development&&webpack",
		"build": "set NODE_ENV=production&&webpack",
		"prepare": "husky install",
		"pre-commit": "lint-staged"
	},
	"lint-staged": {
		"*.scss": [
			"stylelint \"**/*.scss\" --fix"
		],
		"*.js": [
			"prettier --write",
			"node_modules/.bin/eslint --max-warnings=0"
		]
	},
	"browserslist": [
		"last 2 versions",
		"> 1%",
		"IE 10"
	],
	"repository": {
		"type": "git",
		"url": "git+https://github.com/AntoshJkeee/frontend-myself.git"
	},
	"keywords": [],
	"author": "",
	"license": "ISC",
	"bugs": {
		"url": "https://github.com/AntoshJkeee/frontend-myself/issues"
	},
	"homepage": "https://github.com/AntoshJkeee/frontend-myself#readme",
	"devDependencies": {
		"@babel/core": "^7.17.7",
		"@babel/eslint-parser": "^7.17.0",
		"@babel/preset-env": "^7.16.11",
		"babel-loader": "^8.2.3",
		"copy-webpack-plugin": "^10.2.4",
		"cross-env": "^7.0.3",
		"css-loader": "^6.7.1",
		"css-minimizer-webpack-plugin": "^3.4.1",
		"eslint": "^7.32.0",
		"eslint-loader": "^4.0.2",
		"eslint-webpack-plugin": "^3.1.1",
		"html-loader": "^3.1.0",
		"html-webpack-plugin": "^5.5.0",
		"husky": "^7.0.0",
		"lint-staged": "^12.3.7",
		"mini-css-extract-plugin": "^2.6.0",
		"postcss": "^8.4.12",
		"postcss-loader": "^6.2.1",
		"postcss-preset-env": "^7.4.2",
		"prettier": "^2.6.1",
		"pug": "^3.0.2",
		"pug-loader": "^2.4.0",
		"sass": "^1.49.9",
		"sass-loader": "^12.6.0",
		"style-loader": "^3.3.1",
		"stylelint": "^14.6.1",
		"stylelint-declaration-block-no-ignored-properties": "^2.5.0",
		"stylelint-order": "^5.0.0",
		"stylelint-scss": "^4.2.0",
		"terser-webpack-plugin": "^5.3.1",
		"webpack": "^5.70.0",
		"webpack-cli": "^4.9.2",
		"webpack-dev-server": "^4.7.4"
	},
	"dependencies": {
		"normalize.css": "^8.0.1"
	}
}
